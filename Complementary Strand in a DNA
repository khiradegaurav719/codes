#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string s;
	    cin>>s;
	   // int a,t,g,c;
	    for (int i=0; i<n; i++){
	    if(s[i]=='A'){
	        cout<<"T";
	    }
	    else if(s[i]=='T'){
	        cout<<"A";
	    }
	    else if(s[i]=='G'){
	        cout<<"C";
	    }
	    else if(s[i]=='C'){
	        cout<<"G";
	    }}
	    cout<<endl;
	}
	return 0;
}
